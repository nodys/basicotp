{
  "name": "basicnotp",
  "version": "1.0.0",
  "description": "A basic Electron app for 2FA oath",
  "main": "./main.js",
  "scripts": {
    "dev": "mkdir -p build && npm-run-all --parallel dev-statics dev-bundle dev-start",
    "dev-bundle": "mkdir -p build && watchify src/app/app.js --debug --cache-dir ./.cache --watch --verbose -p browserify-hmr -o build/app.js ",
    "dev-statics": "cpx './src/statics/*' ./build -vw",
    "dev-start": "electron .",
    "standard": "standard \"**/*.{vue,js}\""
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "electron": "^1.4.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-runtime": "^6.11.6",
    "babelify": "^7.3.0",
    "bluebird": "^3.4.6",
    "bluebird-co": "^2.1.2",
    "browserify": "^13.1.0",
    "browserify-hmr": "^0.3.5",
    "cpx": "^1.5.0",
    "cross-env": "^2.0.1",
    "domready": "^1.0.8",
    "electron-packager": "^8.0.0",
    "notp": "^2.0.3",
    "npm-run-all": "^3.1.0",
    "standard": "^8.1.0",
    "thirty-two": "^1.0.2",
    "vue": "^2.0.0-rc.6",
    "vueify": "^9.2.4",
    "vuex": "^2.0.0-rc.5",
    "watchify": "^3.7.0"
  },
  "browserify": {
    "transform": [
      "vueify",
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "stage-2"
          ]
        }
      ]
    ]
  },
  "private": true
}
